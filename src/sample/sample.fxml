<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<GridPane alignment="center" hgap="10" stylesheets="@stylesheet.css" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <BorderPane prefHeight="168.0" prefWidth="200.0">
         <center>
            <VBox prefHeight="200.0" prefWidth="95.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="start" mnemonicParsing="false" onAction="#clicked" prefHeight="37.0" prefWidth="187.0" text="Start">
                     <VBox.margin>
                        <Insets bottom="10.0" left="20.0" right="20.0" top="20.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="edit" mnemonicParsing="false" onAction="#clicked" prefHeight="37.0" prefWidth="187.0" text="Edit">
                     <VBox.margin>
                        <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="exit" mnemonicParsing="false" onAction="#clicked" prefHeight="37.0" prefWidth="187.0" text="Exit">
                     <VBox.margin>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="10.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>
</GridPane>
